{% extends 'base.html.twig' %}

{% block title %}Subject{% endblock %}

{% block body %}

    <h1>{{ subject.name }}</h1>

    <ul>
        {% for user in subject.users %}
            {% if app.user != user %}
                <li>{{ user.firstname }} {{ user.lastname }}</li>
            {% endif %}
        {% endfor %}
    </ul>

{% endblock %}
